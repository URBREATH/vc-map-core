{
  "opts": {
    "recurse": true,
    "template": "build/index"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": "\\.js$",
    "include": ["src"],
    "exclude": ["index.js", "src/ol", "src/cesium"]
  },
  "plugins": ["jsdoc-plugin-typescript"],
  "typescript": {
    "moduleRoot": "src"
  }
}
