{
  "opts": {
    "recurse": true,
    "template": "build/index"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": "\\.js$",
    "include":[
      "src"
    ],
    "exclude": [
      "index.js"
    ]
  },
  "plugins": [
    "jsdoc-plugin-typescript",
    "build/index/export-plugin"
  ],
  "typescript": {
    "moduleRoot": "src"
  }
}
