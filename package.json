{
  "name": "@vcmap/core",
  "version": "5.0.0-rc.32",
  "description": "",
  "main": "index.js",
  "type": "module",
  "types": "index.d.ts",
  "scripts": {
    "test": "node node_modules/mocha/bin/_mocha --exit --require tests/setupJsdom.js --require tests/setup.js --file tests/vcs.js --recursive \"tests/**/*spec.js\"",
    "lint:js": "eslint . --ext .vue,.js,.cjs,.mjs",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "format": "prettier --write --list-different . && npm run lint:js -- --fix",
    "pretype-check": "npm run build-typedefs",
    "type-check": "node build/typeCheck.js",
    "postinstall": "node build/postinstall.js",
    "coverage": "c8 --reporter text-summary npm run test",
    "coverage:gitlab": "c8 --reporter text-summary npm run test -- --reporter mocha-junit-reporter",
    "build-docs": "jsdoc -c build/docs/conf.json",
    "build-index": "jsdoc -c build/index/conf.json",
    "build-typedefs": "jsdoc -c build/info/conf.json",
    "build-tsd": "node build/buildDTs.js",
    "prepare": "npm run build-index && npm run build-tsd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualcitySYSTEMS/map-core.git"
  },
  "files": [
    "build/postinstall.js",
    "src/",
    "tests/data/",
    "tests/unit/helpers/",
    "LICENSE.md",
    "README.md",
    "index.js",
    "index.d.ts"
  ],
  "author": "Virtual City Systems",
  "license": "MIT",
  "devDependencies": {
    "@vcsuite/eslint-config": "^3.0.3",
    "@vcsuite/tsd-jsdoc": "^1.0.2",
    "c8": "^7.7.1",
    "canvas": "^2.8.0",
    "chai": "^4.3.4",
    "eslint": "^8.38.0",
    "jsdoc": "^3.6.7",
    "jsdoc-plugin-typescript": "^2.0.6",
    "jsdom": "^16.5.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.2",
    "nock": "^13.2.4",
    "node-fetch": "^3.2.0",
    "prettier": "^2.8.7",
    "resize-observer-polyfill": "1.5.1",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.6.0",
    "tui-jsdoc-template": "^1.2.2",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "@vcmap-cesium/engine": "~2.3.2",
    "ol": "~7.3.0"
  },
  "eslintConfig": {
    "extends": [
      "@vcsuite/eslint-config/vue"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "index.js",
    "*.d.ts",
    "docs"
  ],
  "prettier": "@vcsuite/eslint-config/prettier.js",
  "dependencies": {
    "@types/rbush": "^3.0.0",
    "@vcsuite/check": "^1.1.2",
    "@vcsuite/logger": "^1.0.1",
    "@vcsuite/parsers": "^1.0.1",
    "fast-deep-equal": "^3.1.3",
    "proj4": "^2.7.5",
    "rbush": ">=3.0.1",
    "rbush-knn": "^3.0.1",
    "uuid": "^8.3.2"
  }
}
